@model List<MyApp.Controllers.ExampleUser>
@{
    ViewData["Title"] = "Home Page";
    // var ExampleUsers = ViewBag.ExampleUsers as List<MyApp.Controllers.ExampleUser>;
    // var ExampleUsers = ViewData["ExampleUsers"] as List<MyApp.Controllers.ExampleUser>;
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div>
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
            </tr>
        }
    </table>
</div>
